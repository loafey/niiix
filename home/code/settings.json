        "swordlord",
        "tscn",
        "tuplify",
        "Twople",
        "typeshare",
        "Vecture",
        "Xform"
    ],
    "rust-analyzer.inlayHints.enable": false,
    "rust-analyzer.inlayHints.renderColons": false,
    "rust-analyzer.cargo.buildScripts.enable": true,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "workbench.preferredDarkColorTheme": "Ayu Mirage",
    "workbench.preferredLightColorTheme": "Ayu Light",
    "window.autoDetectColorScheme": true,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "material-icon-theme.folders.color": "#90a4af",
    "material-icon-theme.files.associations": {
        "*.rs": "rescript"
    },
    "latex-workshop.latex.build.forceRecipeUsage": false,
    "workbench.startupEditor": "none",
    "[python]": {
        "editor.formatOnType": true
    },
    "terminal.integrated.profiles.linux": {
        "zsh": {
            "path": "zsh",
            "args": [
                "-l",
                "-i"
            ]
        },
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        }
    },
    "latex-workshop.message.latexlog.exclude": [
        "Some font shapes were not available, defaults substituted.",
        "Font shape `.*' undefined"
    ],
    "rust-analyzer.checkOnSave": true,
    "rust-analyzer.check.command": "clippy",
    "git.openRepositoryInParentFolders": "always",
    "editor.acceptSuggestionOnEnter": "off",
    "errorLens.excludeBySource": [
        "rustc(Click for full compiler diagnostic)"
    ],
    "editor.formatOnSave": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.linkedEditing": true,
    "slint.preview.style": "fluent",
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "breadcrumbs.enabled": true,
    "typst-lsp.exportPdf": "never",
    "haskell.manageHLS": "PATH",
    "keyboard.dispatch": "keyCode",
    "editor.quickSuggestions": {
        "comments": true,
        "strings": true
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "xelatexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "rnw2tex",
            "command": "Rscript",
            "args": [
                "-e",
                "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
            ],
            "env": {}
        },
        {
            "name": "jnw2tex",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"tex\")"
            ],
            "env": {}
        },
        {
            "name": "jnw2texminted",
            "command": "julia",
            "args": [
                "-e",
                "using Weave; weave(\"%DOC_EXT%\", doctype=\"texminted\")"
            ],
            "env": {}
        },
        {
            "name": "pnw2tex",
            "command": "pweave",
            "args": [
                "-f",
                "tex",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "pnw2texminted",
            "command": "pweave",
            "args": [
                "-f",
                "texminted",
                "%DOC_EXT%"
            ],
            "env": {}
        },
        {
            "name": "tectonic",
            "command": "tectonic",
            "args": [
                "--synctex",
                "--keep-logs",
                "%DOC%.tex"
            ],
            "env": {}
        }
    ],
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "agdaMode.connection.agdaLanguageServer": false,
    "agdaMode.connection.commandLineOptions": "-l standard-library -i .",
    "window.commandCenter": false,
    "rust-analyzer.inlayHints.closureStyle": "hide",
    "rust-analyzer.inlayHints.genericParameterHints.const.enable": false,
    "workbench.activityBar.location": "hidden",
    "workbench.editor.showTabs": "none",
    "workbench.sideBar.location": "right",
    "autoHide.ui": {
        "sidebar": true,
        "auxiliaryBar": true,
        "panel": false,
        "references": false,
        "notifications": true,
        "cursor": false
    },
    "editor.inlayHints.enabled": "offUnlessPressed",
    "window.titleBarStyle": "native",
    "remote.SSH.enableRemoteCommand": true,
    "[typst]": {
        "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
    },
    "window.customTitleBarVisibility": "never",
    "remote.SSH.experimental.chat": false,
    "chat.agent.maxRequests": 0,
    "github.copilot.enabled": false,
    "github.copilot.editor.enableCodeActions": false,
    "github.copilot.nextEditSuggestions.enabled": false,
    "chat.commandCenter.enabled": false,
    "chat.agent.enabled": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
    "inlineChat.lineNaturalLanguageHint": false,
    "chat.mcp.discovery.enabled": false,
    "chat.promptFiles": false,
    "chat.promptFilesLocations": {
        ".github/prompts": false
    },
    "chat.setupFromDialog": false,
    "github.copilot.chat.search.semanticTextResults": false,
    "chat.edits2.enabled": false,
    "dev.containers.dockerPath": "podman",
    "accessibility.verbosity.panelChat": false,
    "accessibility.verbosity.inlineChat": false,
    "chat.detectParticipant.enabled": false,
    "chat.extensionTools.enabled": false,
    "chat.mcp.enabled": false,
    "chat.sendElementsToChat.attachCSS": false,
    "chat.sendElementsToChat.attachImages": false,
    "chat.sendElementsToChat.enabled": false,
    "chat.useFileStorage": false,
    "editor.inlineSuggest.enabled": false,
    "harper.path": "/nix/store/1d5hzhnssysrannyqk0m867xbsacd0gh-user-environment/bin/harper-ls",
    "workbench.editor.empty.hint": "hidden",
    "dotnet.codeLens.enableReferencesCodeLens": false
}